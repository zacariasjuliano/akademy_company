<?xml version="1.0"?>
<trytond>
	<data>
        <!-- start RELATÓRIO DA INSTITUIÇÃO -->
            <record model="ir.action.report" id="company_report">
                <field name="name">Instituição</field>
                <field name="report_name">akademy_report.company.report</field>
                <field name="model">company.company</field>
                <field name="report">akademy_company/reports/company.odt</field>
            </record>
            <record model="ir.action.keyword" id="company_report_keyword">
                <field name="keyword">form_print</field>
                <field name="model">company.company,-1</field>
                <field name="action" ref="company_report"/>
            </record>
        <!-- start RELATÓRIO DA INSTITUIÇÃO -->

        <!-- start RELATÓRIO DE FUNCIONÁRIOS -->
            <record model="ir.action.report" id="employee_report">
                <field name="name">Funcionários</field>
                <field name="report_name">akademy_report.company.employee.report</field>
                <field name="model">company.employee</field>
                <field name="report">akademy_company/reports/employee.odt</field>
            </record>
            <record model="ir.action.keyword" id="employee_report_keyword">
                <field name="keyword">form_print</field>
                <field name="model">company.employee,-1</field>
                <field name="action" ref="employee_report"/>
            </record>
        <!-- start RELATÓRIO DE FUNCIONÁRIOS -->

        <!-- start RELATÓRIO DE DISCENTE -->
            <record model="ir.action.report" id="student_report">
                <field name="name">Discente</field>
                <field name="report_name">akademy_report.company.student.report</field>
                <field name="model">company.student</field>
                <field name="report">akademy_company/reports/student.odt</field>
            </record>
            <record model="ir.action.keyword" id="student_report_keyword">
                <field name="keyword">form_print</field>
                <field name="model">company.student,-1</field>
                <field name="action" ref="student_report"/>
            </record>
        <!-- start RELATÓRIO DE DISCENTE -->


        
	</data>
</trytond>
